{
  "name": "website-next",
  "version": "1.0.0",
  "private": true,
  "description": "Red Badger's website",
  "main": "build/server",
  "scripts": {
    "build": "npm run clean && webpack --progress",
    "clean": "rimraf build",
    "lint": "eslint --cache .",
    "local": "HOT=true node server.dev.js",
    "security:check": "./node_modules/.bin/nsp check",
    "start": "node build/server",
    "test": "npm run test:server && npm run test:client && npm run security:check && npm run lint && npm run test:e2e",
    "test:client": "karma start spec/karma.config.js",
    "test:e2e": "wdio spec/wdio.config.js",
    "test:server": "mocha --compilers js:babel-core/register --require spec/mocha-helper.js --recursive --reporter spec $(find src -name *spec.js)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redbadger/website-next.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/redbadger/website-next/issues"
  },
  "homepage": "https://github.com/redbadger/website-next",
  "pre-push": [
    "lint",
    "test:server"
  ],
  "dependencies": {
    "chalk": "^1.1.1",
    "classnames": "^2.2.3",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "html-to-react": "^0.1.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.2.0",
    "moment": "^2.11.1",
    "param-case": "^1.1.2",
    "query-string": "^3.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "4.2.1",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "redux": "3.2.1",
    "redux-devtools": "3.0.2",
    "redux-thunk": "^1.0.3",
    "serialize-javascript": "^1.1.2",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015-webpack": "^6.3.13",
    "babel-preset-node5": "^10.5.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fake-file-loader": "^1.0.0",
    "file-loader": "^0.8.5",
    "glob": "^6.0.4",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "node-hook": "^0.2.0",
    "nodemon": "1.8.1",
    "nsp": "^2.2.0",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-cssnext": "^2.4.0",
    "postcss-discard-duplicates": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "pre-push": "0.1.1",
    "react-addons-test-utils": "^0.14.7",
    "react-shallow-testutils": "^0.7.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.2",
    "redbox-react": "^1.2.2",
    "rimraf": "2.5.1",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webdriverio": "^3.4.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "engines": {
    "node": "5.5.0"
  }
}
